{
  "name": "direct-agentcore-client",
  "version": "1.0.0",
  "description": "AWS Bedrock AgentCoreに直接アクセスするクライアント",
  "main": "direct_agentcore_client.js",
  "type": "module",
  "scripts": {
    "build": "tsc client.ts",
    "mcp:local": "ts-node client.ts --local",
    "mcp:remote": "ts-node client.ts --remote",
    "mcp:local:debug": "ts-node client.ts --local --debug",
    "mcp:remote:debug": "ts-node client.ts --remote --debug",
    "lint": "eslint \"client.ts\" \"src/**/*.ts\"",
    "lint:fix": "eslint \"client.ts\" \"src/**/*.ts\" --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\""
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-bedrock-agentcore": "^3.515.0",
    "@aws-sdk/client-ssm": "^3.873.0",
    "@aws-sdk/client-sts": "^3.873.0",
    "@aws-sdk/credential-provider-node": "^3.873.0",
    "@aws-sdk/node-http-handler": "^3.370.0",
    "@aws-sdk/signature-v4": "^3.370.0",
    "@aws-sdk/types": "^3.862.0",
    "@modelcontextprotocol/sdk": "^1.13.2",
    "@types/node-fetch": "^2.6.13",
    "axios": "^1.11.0",
    "commander": "^11.1.0",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.41.0"
  }
}
